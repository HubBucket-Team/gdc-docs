(function ($) {
	$(document).ready(
		function() {
			$.getJSON("https://gdc-api.nci.nih.gov/v0/notifications", function(b) {
				if(b.data.length > 0) {
					$(".banner-maintenance").css("display", "block");
					$(".banner-maintenance").addClass(()=>"banner-maintenance-"+b.data[0].level.toLowerCase());
					$(".banner-maintenance-message").append(b.data[0].message);
				}
			});
		}
	);
})(jQuery);
